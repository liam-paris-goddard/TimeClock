<?xml version="1.0" encoding="UTF-8" ?>
<controls:BaseContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns:controls="clr-namespace:Goddard.Clock.Controls;assembly=Goddard.Clock"
                          x:Class="Goddard.Clock.Controls.PagedGoddardButtonGrid"
                          xmlns:helpers="clr-namespace:Goddard.Clock.Helpers;assembly=Goddard.Clock">
    <controls:GoddardFrame Grid.Column="1"
                           HasShadow="False"
                           HorizontalOptions="FillAndExpand"
                           VerticalOptions="FillAndExpand"
                           Margin="50,5,50,5">
        <Grid x:Name="buttonGrid"
              HorizontalOptions="FillAndExpand"
              VerticalOptions="FillAndExpand"
              RowSpacing="15"
              ColumnSpacing="5" />
    </controls:GoddardFrame>
</controls:BaseContentView>